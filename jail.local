[ajenti]
enabled = true
port = 8000
filter = ajenti
logpath = /var/log/user.log
maxretry = 3

[mod_qos]
enabled = true
port = 80
filter = mod_qos
logpath = /var/log/apache2/error.log
maxretry = 1

[ovz-wp]
enabled = true
port = 3000
filter = ovz-wp
logpath = /opt/ovz-web-panel/log/production.log
maxretry = 3